{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/Components/*"],
      "@routes/*": ["./src/Routes/*"],
      "@screens/*": ["src/Screens/*"],
      "@storage/*": ["./src/Storage/*"],
      "@theme/*": ["./src/Theme/*"],
      "@utils/*": ["./src/Utils/*"],
      "@service/*": ["./src/Service/*"],
			"@contexts/*": ["./src/Contexts/*"]
    }
  }
}
